# Copy this file to TradingSandboxAPI/.env and update with your values

# Database Configuration
POSTGRES_DB=tradingsandbox
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_URL=jdbc:postgresql://localhost:5432/tradingsandbox

# Admin User (seeded on first startup)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=changeme
ADMIN_FIRST_NAME=Admin
ADMIN_LAST_NAME=User
ADMIN_EMAIL=admin@example.com

# FastAPI Service
FASTAPI_BASE_URL=http://localhost:8000
FASTAPI_ACCESS_KEY=your-access-key-here

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-change-this-in-production
JWT_EXPIRATION_MS=3600000
