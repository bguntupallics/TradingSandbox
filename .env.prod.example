# Production environment variables
# Copy this to .env.prod and fill in the values

# Database
POSTGRES_USER=tradingsandbox
POSTGRES_PASSWORD=your_secure_password_here

# JWT
JWT_SECRET=your_32_char_minimum_jwt_secret_here
JWT_EXPIRATION_MS=3600000

# Admin user
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_admin_password_here
ADMIN_FIRST_NAME=Admin
ADMIN_LAST_NAME=User
ADMIN_EMAIL=admin@yourdomain.com

# FastAPI access key (shared between API and data-acq)
FASTAPI_ACCESS_KEY=your_fastapi_access_key_here

# Alpaca API (for market data)
ALPACA_API_KEY=your_alpaca_key
ALPACA_API_SECRET=your_alpaca_secret

# Frontend API URL (use your server's public IP or domain)
VITE_API_BASE_URL=http://YOUR_SERVER_IP:8080
